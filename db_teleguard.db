CREATE TABLE FUNCIONARIO ( 
    ID_FUNC INTEGER, 
    RG_FUNC    TEXT NOT NULL, 
    NOME_FUNC TEXT NOT NULL, 
    SENHA TEXT NOT NULL, 
   PRIMARY KEY("ID_FUNC" AUTOINCREMENT) 
); 
CREATE TABLE SERVIDOR ( 
    IP_SERV TEXT, 
    DATA_INIC TEXT NOT NULL, 
    HORA_INIC TEXT NOT NULL, 
    DATA_TERM TEXT NOT NULL, 
    HORA_TERM TEXT NOT NULL, 
    ID_FUNC INTEGER NOT NULL, 
    PRIMARY KEY("IP_SERV"), 
    FOREIGN KEY("ID_FUNC") REFERENCES "FUNCIONARIO"("ID_FUNC") 
); 
CREATE TABLE LOGIN ( 
    ID_LOGIN INTEGER, 
    ID_FUNC INTEGER NOT NULL, 
    IP_SERV TEXT NOT NULL, 
    DATA_LOGIN TEXT NOT NULL, 
    HORA_LOGIN TEXT NOT NULL, 
    PRIMARY KEY("ID_LOGIN" AUTOINCREMENT), 
    FOREIGN KEY("ID_FUNC") REFERENCES "FUNCIONARIO"("ID_FUNC"), 
    FOREIGN KEY("IP_SERV") REFERENCES "SERVIDOR"("IP_SERV") 
); 
CREATE TABLE COMPUTADOR ( 
    ID_COMP INTEGER, 
    IP_SERV TEXT NOT NULL, 
    MARCA_COMP TEXT NOT NULL, 
    SO_COMP TEXT NOT NULL, 
    COR_COMP TEXT NOT NULL, 
    PRIMARY KEY("ID_COMP" AUTOINCREMENT), 
    FOREIGN KEY("IP_SERV") REFERENCES "SERVIDOR"("IP_SERV") 
); 
CREATE TABLE CATEG_SITE ( 
    ID_CATEG INTEGER PRIMARY KEY AUTOINCREMENT, 
    DESCRICAO TEXT NOT NULL
); 
CREATE TABLE SITES_PROIBIDOS ( 
    ID_SITE INTEGER, 
    ID_CATEG INTEGER NOT NULL, 
    NOME_SITE TEXT NOT NULL, 
    URL_SITE TEXT NOT NULL, 
    PRIMARY KEY("ID_SITE" AUTOINCREMENT), 
    FOREIGN KEY("ID_CATEG") REFERENCES "CATEG_SITE"("ID_CATEG") 
); 
CREATE TABLE INFRACAO ( 
    ID_INFRA INTEGER, 
    ID_COMP INTEGER NOT NULL, 
    ID_SITE INTEGER NOT NULL, 
    DATA_INFRA TEXT NOT NULL, 
    HORA_INFRA TEXT NOT NULL, 
    CAMINHO_VIDEO TEXT NOT NULL, 
    PRIMARY KEY("ID_INFRA" AUTOINCREMENT), 
    FOREIGN KEY("ID_COMP") REFERENCES "COMPUTADOR"("ID_COMP"), 
    FOREIGN KEY("ID_SITE") REFERENCES "SITES_PROIBIDOS"("ID_SITE") 
); 
 
CREATE TABLE HISTO_INFRA ( 
    ID_HISTO INTEGER, 
    IP_SERV TEXT NOT NULL, 
    ID_INFRA INTEGER NOT NULL, 
    PRIMARY KEY("ID_HISTO" AUTOINCREMENT), 
    FOREIGN KEY("ID_INFRA") REFERENCES "INFRACAO"("ID_INFRA"), 
    FOREIGN KEY("IP_SERV") REFERENCES "SERVIDOR"("IP_SERV") 
); 
